{
  "name": "timothie-jewelry-customizer",
  "version": "1.0.0",
  "description": "Drag-and-drop jewelry customizer for Timothie & Co",
  "main": "src/js/main.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "playwright test --config=tests/config/playwright.visual.config.js",
    "test:performance": "jest --testPathPattern=tests/performance",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "lint": "eslint src/js/**/*.js",
    "lint:fix": "eslint src/js/**/*.js --fix"
  },
  "keywords": ["jewelry", "customizer", "drag-drop", "konva", "canvas"],
  "author": "Timothie & Co",
  "license": "MIT",
  "dependencies": {
    "konva": "^9.2.0",
    "@supabase/supabase-js": "^2.38.0"
  },
  "devDependencies": {
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "html-webpack-plugin": "^5.5.0",
    "css-loader": "^6.8.0",
    "style-loader": "^3.3.0",
    "file-loader": "^6.2.0",
    "babel-loader": "^9.1.0",
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "eslint": "^8.44.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "jest-canvas-mock": "^2.5.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/user-event": "^14.5.1",
    "@playwright/test": "^1.40.0",
    "playwright": "^1.40.0",
    "jest-canvas-mock": "^2.5.2",
    "jsdom": "^23.0.1",
    "@supabase/supabase-js": "^2.38.0",
    "dotenv": "^16.3.1",
    "cross-env": "^7.0.3",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "jest-junit": "^16.0.0",
    "wait-on": "^7.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}